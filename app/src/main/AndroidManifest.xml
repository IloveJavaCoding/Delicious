        <activity android:name="com.example.delicious.Order_state" />
        <activity android:name="com.example.delicious.Check.Check" />
        <activity android:name="com.example.delicious.Cart.Cart" />
        <activity android:name="com.example.delicious.ui.common.Restaurant1" />
        <activity android:name="com.example.delicious.ui.common.Homepage" />
        <activity android:name=".ui.common.sign_out.Logout3" />
        <activity android:name=".ui.common.sign_out.Logout2" />
        <activity android:name=".ui.common.sign_out.Logout1" />
        <activity android:name=".ui.common.sign_in.Registerpag" />
        <activity android:name=".ui.common.sign_in.Loginpag" />
        <activity
            android:name=".ui.common.sign_in.MainActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>