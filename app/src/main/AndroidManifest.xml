<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.delicious"
    android:targetSandboxVersion="1">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name="com.example.delicious.Self_class.Controls"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="com.example.delicious.ui.common.order.OrderList"></activity>
        <activity android:name="com.example.delicious.Home.Loading" />
        <activity android:name=".ui.common.sign_in.Forgetpage" />
        <activity android:name="com.example.delicious.History.Order_history" />
        <activity android:name="com.example.delicious.ui.common.order.MyOrder" />
        <activity android:name="com.example.delicious.ui.common.Restaurant2" />
